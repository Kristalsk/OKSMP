  object qryAddChild: TFDCommand
    Connection = DataModule1.FDConnection
    Transaction = trWrite
    CommandText.Strings = (
      'EXECUTE PROCEDURE sp_add_child('
      '  NEXT VALUE FOR gen_new_table_id, '
      '  :SURNAME, '
      '  :FIRSTNAME,'
      '  :LASTNAME,'
      '  :BIRTHDAY,'
      '  :GENDER'
